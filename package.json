{
  "name": "@ai-nx-starter/source",
  "version": "1.7.1",
  "license": "MIT",
  "scripts": {
    "ui": "npx nx serve web-ui",
    "server": "npx nx serve web-server",
    "build": "nx run-many --target=build --all --parallel=5",
    "build:prod": "nx run-many --target=build --all --parallel=5 --configuration=production",
    "start": "npx nx run-many --target=serve --projects=web-server,web-ui --parallel=2 --output-style=stream",
    "gen-api-client": "node --loader ts-node/esm tools/api-client-generator/src/generate.ts && npm run format:fix -- packages/api-client/src/api/**/*.ts",
    "test": "nx run-many --target=test --all --parallel=5",
    "test:watch": "nx run-many --target=test --all --parallel=1 --watch",
    "test:coverage": "nx run-many --target=test --all --parallel=5 --coverage",
    "e2e": "npx nx e2e web-ui-e2e",
    "e2e:headed": "npx nx e2e-headed web-ui-e2e",
    "e2e:ui": "npx nx e2e-ui web-ui-e2e",
    "lint": "nx run-many --target=lint --all --parallel=5",
    "lint:fix": "nx run-many --target=lint:fix --all --parallel=5",
    "format:check": "prettier --check \"**/*\"",
    "format:fix": "prettier --write \"**/*\"",
    "clean": "rm -rf .nx tmp node_modules dist .angular",
    "clean:deep": "pnpm -r exec rm -rf node_modules dist && rm -rf .nx tmp node_modules dist .angular"
  },
  "private": true,
  "packageManager": "pnpm@10.20.0",
